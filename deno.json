{
  "imports": {
    "@optimizely/optimizely-sdk": "https://esm.sh/@optimizely/optimizely-sdk@6.0.0/dist/index.universal.es.min.js",
    "json-schema": "https://esm.sh/json-schema@0.4.0"
  },
  "tasks": {
    "test": "deno test",
    "test:unit": "deno test *.test.ts",
    "test:integration": "deno run --allow-net integration.test.ts",
    "dev": "deno run --allow-net server.ts",
    "start": "deno run --allow-net server.ts"
  },
  "test": {
    "include": ["**/*.test.ts"],
    "exclude": ["integration.test.ts"]
  },
  "compilerOptions": {
    "lib": ["deno.window"],
    "strict": true
  }
}
